{
  "name": "unified-redux-wrapper",
  "version": "1.3.4",
  "description": "A wrapper around redux , used in conjuction with react to make incorporating state management in your applications easy as a breeze ",
  "main": "build/index.js",
  "scripts": {
    "build": "rm -rf build && babel src -d build",
    "test": "jest",
    "coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "humps": "^2.0.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0"
  },
  "bugs": {
    "url": "https://github.com/felixsonyusuftosin/redux-wrapper/issues",
    "email": "felixsonyusuftosin@gmail.com"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|css|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js"
    },
    "moduleFileExtensions": [
      "js",
      "js.map",
      "*.js",
      "*.js.map"
    ]
  },
  "keywords": [
    "redux",
    "unified-redux",
    "react-redux",
    "redux-wrapper",
    "one-store"
  ],
  "contributors": [
    {
      "name": "Ikechukwu Njoku",
      "email": "ikeenjoku@gmail.com",
      "url": "https://github.com/ikenjoku"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/felixsonyusuftosin/redux-wrapper.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "7.7.0",
    "@babel/preset-env": "7.7.1",
    "@types/jest": "24.0.15",
    "@types/jest-cli": "23.6.0",
    "@types/node-fetch": "^2.5.3",
    "@types/redux-mock-store": "^1.0.1",
    "babel-loader": "8.0.6",
    "cross-env": "5.2.0",
    "fetch-mock": "7.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "^24.8.0",
    "jest-cli": "24.8.0",
    "node-fetch": "^2.6.0",
    "redux-mock-store": "^1.5.3",
    "url-search-params-polyfill": "2.0.3",
    "webpack": "4.41.2"
  },
  "author": "Felixson Yusuf Tosin",
